{
  "version": 2,
  "builds": [
    {
      "src": "shanyraq_project/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "shanyraq_project/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "shanyraq_project.settings",
    "SECRET_KEY": "django-insecure-замени_на_свой",
    "DEBUG": "False",
    "ALLOWED_HOSTS": ".vercel.app,127.0.0.1,localhost"
  }
}